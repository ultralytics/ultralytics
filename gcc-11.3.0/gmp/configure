	path="s390_32/esame/$cpu s390_32/esame s390_32"
	gcc_cflags_arch="-march=$gccarch"
	cat >>confdefs.h <<_ACEOF
#define HAVE_HOST_CPU_s390_$cpu 1
_ACEOF

	$as_echo "#define HAVE_HOST_CPU_s390_zarch 1" >>confdefs.h

	extra_functions=""
        ;;
      z9 | z9esa)
        cpu="z9"
	gccarch="z9-109"
	path="s390_32/esame/$cpu s390_32/esame s390_32"
	gcc_cflags_arch="-march=$gccarch"
	cat >>confdefs.h <<_ACEOF
#define HAVE_HOST_CPU_s390_$cpu 1
_ACEOF

	$as_echo "#define HAVE_HOST_CPU_s390_zarch 1" >>confdefs.h

	extra_functions=""
        ;;
      z10 | z10esa)
