{
  "name": "qualitative",
  "source_weights_path": "./models/8s_300e_128b.pt",
  "source_stream_path": "./videos/13.mp4",
  "stabilize": false,
  "logging": false,
  "stream_mode": false,

  "device": "cuda",
  "video_stride": 1,

  "save_dir": "./outputs/",
  "save_results": false,
  "save_video": false,
  "compression_mode": true,
  "display": false,

  "img_size": 640,
  "conf_threshold": 0.1,
  "iou_threshold": 0.7,
  "max_det": 50,

  "track_high_thresh": 0.4,
  "track_low_thresh": 0.1,
  "new_track_thresh": 0.25,
  "track_buffer": 200,
  "match_thresh": 0.9,

  "proximity_thresh": 0.9,
  "appearance_thresh": 0.8,
  "with_reid": false,
  "reid_default": true,
  "weight_path": "ver12.pt",

  "gmc_method": null,

  "action_recognition": {
    "enabled": true,
    "speed_projection": [0.7, 1.5],
    "_comment": "",

    "gather": {
      "enabled": true,
      "distance_threshold": 1.0,
      "area_threshold": 0.5,
      "_comment": ""
    },

    "stand_still": {
      "enabled": true,
      "speed_threshold": 0.001,
      "_comment": ""
    },

    "fast_approach": {
      "enabled": true,
      "draw": true,
      "distance_threshold": 4,
      "speed_threshold": 0.005,
      "_comment": ""
    },

    "suddenly_run": {
      "enabled": true,
      "speed_threshold": 0.025,
      "_comment": ""
    },

    "overstep_boundary": {
      "enabled": true,
      "draw": true,
      "line": [0, 0, 1920, 1080],
      "region": "top"
    }
  }
}